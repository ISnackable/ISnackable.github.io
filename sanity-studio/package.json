{
  "name": "isnackable-studio",
  "private": true,
  "version": "1.0.0",
  "description": "ISnackable Sanity Studio",
  "main": "package.json",
  "author": "ISnackable",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "eval $(grep '^SANITY_STUDIO_API_*' .env) sanity start",
    "format": "prettier-eslint --write \"**/*.js\" \"!node_modules/**\"",
    "build": "sanity build",
    "graphql-deploy": "eval $(grep '^SANITY_STUDIO_API_*' .env) sanity graphql deploy --playground",
    "lint": "eslint .",
    "test": "sanity check"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@sanity/base": "^2.28.2",
    "@sanity/code-input": "^2.28.2",
    "@sanity/core": "^2.28.1",
    "@sanity/dashboard": "^2.28.2",
    "@sanity/default-layout": "^2.28.2",
    "@sanity/default-login": "^2.28.2",
    "@sanity/desk-tool": "^2.28.2",
    "@sanity/production-preview": "^2.15.0",
    "@sanity/vision": "^2.28.2",
    "prop-types": "^15.8.0",
    "react": "^17.0",
    "react-dom": "^17.0",
    "react-icons": "^4.3.1",
    "sanity-plugin-asset-source-unsplash": "^0.1.5",
    "sanity-plugin-computed-field": "^1.0.2",
    "sanity-plugin-dashboard-widget-document-list": "^0.0.13",
    "sanity-plugin-media": "^1.4.3",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@sanity/cli": "^2.28.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ISnackable/isnackable.me.git"
  }
}
